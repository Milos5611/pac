{
  "name": "api",
  "version": "1.0.0",
  "description": "api",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "dev": "nodemon -e 'js,ts' -w src -d 2 -x 'lb-tsc && node --inspect=9202 -r source-map-support/register . || exit 1'",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t pac_mnikolic_api .",
    "docker:run": "docker run -p 3000:3000 -d pac_mnikolic_api",
    "migrate": "node -r dotenv/config ./dist/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "seed": "node -r dotenv/config ./dist/helper/seed-dev.script.js",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^6.0.1",
    "@loopback/boot": "^2.4.1",
    "@loopback/core": "^2.9.3",
    "@loopback/extension-logging": "^0.3.11",
    "@loopback/extension-metrics": "^0.3.13",
    "@loopback/graphql": "^0.1.0",
    "@loopback/repository": "^2.11.0",
    "@loopback/rest-explorer": "^2.2.8",
    "@loopback/service-proxy": "^2.3.6",
    "@okta/okta-sdk-nodejs": "^4.1.1",
    "@types/basic-auth": "^1.1.3",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql": "^14.5.0",
    "@types/uuid": "^8.3.0",
    "apollo-server-express": "^2.17.0",
    "basic-auth": "^2.0.1",
    "class-transformer": "^0.3.1",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "dotenv-extended": "^2.8.0",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "faker": "^5.1.0",
    "graphql": "^15.3.0",
    "loopback-connector-postgresql": "^5.0.2",
    "oasgraph": "^0.14.3",
    "openapi-to-graphql": "^2.2.4",
    "request-promise-native": "^1.0.9",
    "tslib": "^2.0.0",
    "type-graphql": "^1.0.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.0",
    "@loopback/eslint-config": "^9.0.0",
    "@loopback/repository": "^2.10.0",
    "@loopback/rest": "^5.2.1",
    "@loopback/testlab": "^3.2.2",
    "@types/debug": "^4.1.5",
    "@types/faker": "^4.1.12",
    "@types/node": "^10.17.28",
    "eslint": "^7.6.0",
    "jwt-simple": "^0.5.6",
    "moment": "^2.27.0",
    "njwt": "^1.0.0",
    "nodemon": "^2.0.4",
    "source-map-support": "^0.5.19",
    "typescript": "~3.9.7"
  }
}
